```prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

enum Difficulty {
  easy
  medium
  hard
}

model Question {
  id                Int      @id @default(autoincrement())
  category          String
  type              String
  difficulty        Difficulty
  question          String   @unique
  correct_answer    String
  incorrect_answers Json
  createdAt         DateTime @default(now())
}
```